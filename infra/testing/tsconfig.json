{
    "compilerOptions": {
        "target": "ES2015",
        "lib": [
            "ES2015",
            "DOM"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": [
            "k6"
        ]
    },
    "include": [
        "*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}