up:
  npm run dev -- --open --host 0.0.0.0

generate-api-types:
  node scripts/generate-api-types.js

lint *ARGS: format
        npm run lint {{ARGS}} | tee ui-lint.log

run *ARGS:
    npm run dev -- --host

