up:
  npm run dev -- --open --host 0.0.0.0


lint *ARGS:
        npm run lint {{ARGS}} | tee ui-lint.log

generate-api-types:
  node scripts/generate-api-types.js


build:
  ~/.local/bin/nerdctl build . -t symbology-ui:latest
  ~/.local/bin/nerdctl save symbology-ui:latest -o symbology-ui-latest.tar
