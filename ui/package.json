{
    "name": "symbology-ui",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "lint": "eslint 'src/**/*.{js,ts,svelte}'",
        "format": "prettier --write 'src/**/*.{js,ts,svelte,css,html}'",
        "generate-api-types": "node ./scripts/generate-api-types.js",
        "build:prod": "npm run check && npm run lint && npm run build",
        "serve": "vite preview --host 0.0.0.0 --port 4173",
        "docker:build": "docker build -t symbology-ui .",
        "docker:run": "docker run -p 8080:80 symbology-ui"
    },
    "devDependencies": {
        "@eslint/js": "^8.56.0",
        "@types/marked": "^5.0.2",
        "@types/node": "^22.14.1",
        "eslint": "^8.56.0",
        "prettier": "^3.2.4",
        "svelte": "5.27.0",
        "svelte-eslint-parser": "v1.1.2",
        "terser": "^5.43.1",
        "tslib": "^2.6.2",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.35.1",
        "vite": "v6.3.0"
    },
    "dependencies": {
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "@tsconfig/svelte": "^5.0.4",
        "@types/dompurify": "^3.0.5",
        "dompurify": "^3.2.5",
        "eslint-plugin-svelte": "^3.5.1",
        "loglevel": "^1.9.2",
        "marked": "^16.0.0",
        "prettier-plugin-svelte": "^3.3.3",
        "svelte-check": "^4.1.6"
    }
}